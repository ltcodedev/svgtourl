{
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "preserve",
    "esModuleInterop": true,
    "sourceMap": true,
    "allowJs": true,
    "lib": [
      "es6",
      "dom"
    ],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": [
      "vue-sweetalert2",
    ],
    "rootDir": ".",
    "baseUrl": ".",
    "paths": { 
      "~/*": ["./src/*"],
      "src/*": ["./src/*"]
    }
  }
}